<% if (links.length > 0){ %>
  <li class="tweet">
    <img class="profile-pic" src="<%= target_profile_image_url %>" alt="">
    <div class="tweet-content">
      <p>

        <h3>
        <%= target_name %> posted <a href="<%= links[0].url %>" class="test"><%= links[0].title %></a></h3>
        <h4><p><%= text %></p></h4>
        <p>tweeted on <%= created_at %></p>

      </h1>
      </p>
      <ul class="result-keywords">
          <% for (var i=0; i< text_keywords.length; i++) { %>
            <li><input class="keyword" type="submit" value="<%=text_keywords[i]%>"></li>
        <% } %>
      </ul>
       <ul class="tweet-keywords">
          <% for (var i=0; i< title_keywords.length; i++) { %>
            <li><input class="keyword" type="submit" value="<%=title_keywords[i]%>"></li>
        <% } %>
      </ul>
    </div>
  </li>
<% } %>
